{
  "rewrites": [
    {
      "source": "/images/:path*",
      "destination": "https://crocoder-company-blog.vercel.app/images/:path*"
    },
    {
      "source": "/_vercel/:path*",
      "destination": "https://crocoder-company-blog.vercel.app/_vercel/:path*"
    },
    {
      "source": "/_astro/:path*",
      "destination": "https://crocoder-company-blog.vercel.app/_astro/:path*"
    },
    {
      "source": "/blog/:path*",
      "destination": "https://crocoder-company-blog.vercel.app/:path*"
    },
    {
      "source": "/rss.xml",
      "destination": "https://crocoder-company-blog.vercel.app/rss.xml"
    },
    {
      "source": "/feed",
      "destination": "https://crocoder-company-blog.vercel.app/rss.xml"
    },
    {
      "source": "/sitemap",
      "destination": "https://crocoder-company-blog.vercel.app/sitemap.xml"
    }
  ],
  "build": {
    "outputDirectory": "dist"
  },
  "redirects": [
    {
      "source": "/sitemap",
      "destination": "/sitemap.xml"
    }
  ]
}
